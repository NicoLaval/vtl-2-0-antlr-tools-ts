{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2017",
    "lib": ["es2015", "DOM"],
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./dist",
    "sourceMap": true,
    "newLine": "LF",
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "incremental": true,
    "strict": true,
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "strictPropertyInitialization": false
  },
  "include": ["src"]
}
